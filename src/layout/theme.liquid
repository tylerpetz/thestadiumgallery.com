<!DOCTYPE html>
<!--[if IE 7]><html lang="en" class="ie7 {% unless settings.responsive_design %}no-{% endunless %}responsive{% if settings.page_direction == 'RTL' %} rtl{% endif %}"{% if settings.page_direction == 'RTL' %} dir="rtl"{% endif %}><![endif]-->
<!--[if IE 8]><html lang="en" class="ie8 {% unless settings.responsive_design %}no-{% endunless %}responsive{% if settings.page_direction == 'RTL' %} rtl{% endif %}"{% if settings.page_direction == 'RTL' %} dir="rtl"{% endif %}><![endif]-->
<!--[if IE 9]><html lang="en" class="ie9 {% unless settings.responsive_design %}no-{% endunless %}responsive{% if settings.page_direction == 'RTL' %} rtl{% endif %}"{% if settings.page_direction == 'RTL' %} dir="rtl"{% endif %}><![endif]-->
<!--[if !IE]><!--><html lang="en" class="{% unless settings.responsive_design %}no-{% endunless %}responsive{% if settings.page_direction == 'RTL' %} rtl{% endif %}"{% if settings.page_direction == 'RTL' %} dir="rtl"{% endif %}><!--<![endif]-->
<head>
  <meta charset="utf-8" />
  <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1' /><![endif]-->
  {%- if settings.responsive_design -%}<meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1" />{%- endif -%}
  {%- if canonical_url != '' -%}<link rel="canonical" href="{{ canonical_url }}" />{%- endif -%}
  {%- if settings.favicon -%}<link rel="shortcut icon" href="{{ 'favicon.png' | asset_url }}" type="image/x-icon" />{%- endif -%}
  {%- if settings.iphone_icon -%}<link rel="apple-touch-icon-precomposed" href="{{ 'iphone_icon.png' | asset_url }}" />{%- endif -%}
  {%- if settings.iphone_icon_retina -%}<link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ 'iphone_icon_retina.png' | asset_url }}" />{%- endif -%}
  {%- if settings.ipad_icon -%}<link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ 'ipad_icon.png' | asset_url }}" />{%- endif -%}
  {%- if settings.ipad_icon_retina -%}<link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ 'ipad_icon_retina.png' | asset_url }}" />{%- endif -%}
  {%- if page_description or page_description != '' -%}<meta name="description" content="{{ page_description }}" />{%- endif -%}
  {%- if template contains 'index' -%}{%- if page_title != blank and page_title != 'Welcome' -%}<title>{{ page_title }}</title>{%- else -%}<title>{{ shop.name }}</title>{%- endif -%}{%- elsif template == '404' -%}<title>Page Not Found | {{ shop.name }}</title>{%- else -%}<title>{{ page_title }} | {{ shop.name }}</title>{%- endif -%}
  {%- include 'fb-open-graph' -%}
  {%- assign GENERAL_FONT = settings.general_font | default:'Poppins' -%}
  {%- assign HEADING_FONT = settings.heading_font | default:'Poppins' -%}
  {%- assign SPECIAL_FONT = settings.special_font | default:'Poppins' -%}
  {%- if HEADING_FONT != GENERAL_FONT -%}<link href="//fonts.googleapis.com/css?family={{HEADING_FONT | replace: ' ', '+'}}:300italic,400italic,500italic,600italic,700italic,800italic,100,200,300,400,500,600,700,800&subset=cyrillic-ext,greek-ext,latin,latin-ext,cyrillic,greek,vietnamese" rel='stylesheet' type='text/css' />{%- endif -%}
  {%- if SPECIAL_FONT != GENERAL_FONT and SPECIAL_FONT != HEADING_FONT -%}<link href="//fonts.googleapis.com/css?family={{SPECIAL_FONT | replace: ' ', '+'}}:300italic,400italic,500italic,600italic,700italic,800italic,100,200,300,400,500,600,700,800&subset=cyrillic-ext,greek-ext,latin,latin-ext,cyrillic,greek,vietnamese" rel='stylesheet' type='text/css' />{%- endif -%}
  <link href="//fonts.googleapis.com/css?family={{GENERAL_FONT | replace: ' ', '+'}}:300italic,400italic,500italic,600italic,700italic,800italic,100,200,300,400,500,600,700,800&subset=cyrillic-ext,greek-ext,latin,latin-ext,cyrillic,greek,vietnamese" rel='stylesheet' type='text/css' />
  {%- include 'social-meta-tags' -%}
  {{- 'rt.application.scss.css' | asset_url | stylesheet_tag -}}
  {{- 'rt.custom.scss.css' | asset_url | stylesheet_tag -}}
  {{- 'custom_code.scss.css' | asset_url | stylesheet_tag -}}

  {%- include 'head_script' -%}
  {{- 'https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.min.js' | script_tag -}}
  {{ content_for_header }}
</head>
{%- include 'modules-init' -%}
{% capture body_class %}{% if settings.page_width == '3'  %} body-full-width{% endif %}{% if settings.advanced_product_list != '' %} product-list-type-{{settings.advanced_product_list}}{% endif %}{% if settings.advanced_product_grid != '' %} product-grid-type-{{settings.advanced_product_grid}}{% endif %}{% if settings.advanced_dropdown_menu != '' %} dropdown-menu-type-{{settings.advanced_dropdown_menu}}{% endif %}{% if settings.advanced_product_button != '' %} products-buttons-action-type-{{settings.advanced_product_button}}{% endif %}{% if settings.advanced_prev_next != '' %} buttons-prev-next-type-{{settings.advanced_prev_next}}{% endif %}{% if settings.advanced_input != '' %} inputs-type-{{settings.advanced_input}}{% endif %}{% if settings.advanced_myaccount != '' %} my-account-type-{{settings.advanced_myaccount}}{% endif %}{% if settings.advanced_cart_block != '' %} cart-block-type-{{settings.advanced_cart_block}}{% endif %}{% if settings.advanced_top_bar != '' %} top-bar-type-{{settings.advanced_top_bar}}{% endif %}{% if settings.advanced_product_page != '' %} product-page-type-{{settings.advanced_product_page}}{% endif %}{% if settings.advanced_megamenu_type != '' %} megamenu-type-{{settings.advanced_megamenu_type}}{% endif %}{% if settings.advanced_search != '' %} search-type-{{settings.advanced_search}}{% endif %}{% if settings.advanced_megamenu_label != '' %} megamenu-label-type-{{settings.advanced_megamenu_label}}{% endif %}{% if settings.advanced_box == '7' %} box-type-4{% else %} no-box-type-7{% endif %}{% if settings.advanced_box != '' %} box-type-{{settings.advanced_box}}{% endif %}{% if settings.advanced_header_margin_top != '' %} header-margin-top-{{settings.advanced_header_margin_top}}{% endif %}{% if settings.advanced_sale_new != '' %} sale-new-type-{{settings.advanced_sale_new}}{% endif %}{% if settings.advanced_button != '' %} button-body-type-{{settings.advanced_button}}{% endif %}{% if settings.advanced_countdown != '' %} countdown-special-type-{{settings.advanced_countdown}}{% endif %}{% if settings.advanced_footer != '' %} footer-type-{{settings.advanced_footer}}{% endif %}{% if settings.advanced_breadcrumb != '' %} breadcrumb-type-{{settings.advanced_breadcrumb}}{% endif %}{% if settings.advanced_border_width != '' %} border-width-{{settings.advanced_border_width}}{% endif %}{% if settings.body_background_color == '#ffffff' or settings.main_background_color == settings.body_background_color or settings.main_background_color == 'rgba(0,0,0,0)' %} body-white{% else %} body-other{% endif %}{% if settings.main_background_color == 'rgba(0,0,0,0)' %} body-white-type-2{% endif %}{% if settings.main_background_color == 'rgba(0,0,0,0)' and settings.box_product_bg == '#ffffff' %} body-white-type-3{% endif %}{% if settings.advanced_banner_hover != '' %} banners-effect-{{settings.advanced_banner_hover}}{% endif %}{{' body-header-type-' | append: settings.header_type}}{% if settings.header_always_show_vermenu_cat %} show-vertical-megamenu-category-page{% endif %}{% if settings.header_always_show_vermenu %} show-vertical-megamenu{% endif %}{% endcapture %}
<body class="{% if template == 'index' %}common-home {% endif %}template{{ template | split: '.' | first | camelize }}{% if template == 'page.faq' %} module-faq{% endif %}{% if template == 'collection' or template contains 'collection.' %}{% if settings.collection_grid_list == 'list' %} cat-list{% endif %}{% endif %}{{body_class}}{%- if settings.product_mobile_variant == '1' and template contains 'product' or template contains 'cart' -%}{{" has-variant-mobile"}}{%- endif -%}{%- if template contains 'cart' and cart.item_count <= 0 -%}{{" no-cart-item-body"}}{%- endif-%}{%- if settings.product_design == "scroll" -%}{{- " fastor-product-design-sticky" -}}{%- endif -%}">
  {%- capture container_class -%}{% if settings.main_layout == '1' or settings.main_layout == '5' %}{{'standard-body'}}{% else %}{{'fixed-body'}}{% endif %}{% if settings.main_layout == '7' %}{{' fixed-body-shoes'}}{% endif %}{% if settings.main_layout == '4' or settings.main_layout == '6' %}{{' fixed-body-2'}}{% endif %}{% if settings.main_layout == '5' %}{{' fixed-body-2-2'}}{% endif %}{% if settings.main_layout == '3' %}{{' with-shadow'}}{% endif %}{%- endcapture -%}
  {%- capture main_class -%}{% if settings.main_layout == '4' %}{{'main-fixed2 main-fixed'}}{% elsif settings.main_layout == '6' %}{{'main-fixed2 main-fixed3 main-fixed'}}{% elsif settings.main_layout != '1' and settings.main_layout != '5' %}{{'main-fixed'}}{% endif %}{%- endcapture -%}
  {%- capture content_class -%}{% if settings.content_layout == '1' %}{{'full-width'}}{% elsif settings.content_layout == '4' %}{{'fixed2 fixed3'}}{% elsif settings.content_layout == '3' %}{{'fixed2'}}{% else %}{{'fixed'}}{% endif %}{%- endcapture -%}
  {%- if settings.product_mobile_variant == '1' and template contains 'product' or template contains 'cart' -%}
  {%- section "product-variants-mobile" -%}
  {%- endif -%}
  {%- include 'header-notice' -%}
  {%- include 'top-promo' -%}
  <div class="{{container_class}}">
    <div id="main" class="{{main_class}}">
      {%- comment -%} Header {%- endcomment -%}
      {%- include 'header' -%}

      {%- section 'mobile-nav' -%}
      {%- comment -%} Breadcrumb {%- endcomment -%}
      {%- if template != 'index' -%}
      {%- include 'breadcrumb' -%}
      {%- endif -%}
      
      {%- comment -%} Main Content {%- endcomment -%}
      <div class="main-content {{content_class}}{%- if template == 'index' -%}{{- " home" -}}{%- endif -%}">
        <div class="background-content"></div>
        <div class="background">
          <div class="shadow"></div>
          <div class="pattern">
 			{%- if template == 'page.about' -%}
            <div class="container">
              <div class="row">
                <div class="col-md-12">
                  <div class="row">
                    {{content_for_layout}}
                  </div>
                </div>
              </div>
            </div> 
            {%- else -%}  
            <div class="container">
              <div class="row">
                <div class="col-md-12">
                  <div class="row">
                    {{content_for_layout}}
                  </div>
                  {%- if template == 'product' -%}
                  	{%- include 'product-bottom' -%}
                  {%- endif -%}
                </div>
              </div>
            </div> 
            {%- endif -%} 
          </div>
        </div>
      </div>
      {%- section "preface-footer" -%}
      {%- include 'footer' -%}
    </div>
    {%- include 'social-widget' -%}
  </div>
  {%- include 'off-canvas-layer' -%}
  {%- include 'extension-notify' -%}
  {%- include 'extension-mailchimp' -%}
  {%- include 'extension-cookie' -%}
  {%- include 'notify-template' -%}
  {%- include 'photoswipe' -%}
  {%- if template == 'page.contact2' -%}{{ '//maps.google.com/maps/api/js?sensor=false&key=' | append: settings.ggm_api_key | script_tag }}{{ 'jquery.gmap.min.js' | asset_url | script_tag }}{%- endif -%}
  {%- if template == 'index' -%}{{ 'jquery.zoom.min.js' | asset_url | script_tag }}{%- endif -%}
  {%- if template == 'page.about' -%}{{ 'jquery.zoom.min.js' | asset_url | script_tag }}{%- endif -%}
  {%- if template == 'index' -%}{{ 'index.js' | asset_url | script_tag }}{%- endif -%}
  {%- if template == 'page.about' -%}{{ 'index.js' | asset_url | script_tag }}{%- endif -%}
  {%- if template == 'blog.grid' or template == 'blog.grid_3_columns' -%}{{ 'masonry.pkgd.min.js' | asset_url | script_tag }}{%- endif -%}
  {%- if template == 'collection' or template contains 'collection.' -%}{{ 'jquery.history.min.js' | asset_url | script_tag }}{%- endif -%}
  {%- if template == 'product' and settings.product_image_zoom != '2' -%}{{ 'jquery.elevateZoom.min.js' | asset_url | script_tag }}{%- endif -%}
  {%- if template == 'product' and settings.product_bottom_viewed -%}{{ 'jquery.products.min.js' | asset_url | script_tag }}{%- endif -%}
  {%- if settings.show_multiple_currencies -%}{{ '//cdn.shopify.com/s/javascripts/currencies.js' | script_tag }}{%- endif -%}
  {{ 'api.jquery.js' | shopify_asset_url | script_tag }}
  {%- include 'api.jquery.custom' -%}
  {{ 'option_selection.js' | shopify_asset_url | script_tag }}
  {{ 'rt.application.js' | asset_url | script_tag }}
  {{ 'theme.js' | asset_url | script_tag }}
</body>
</html>