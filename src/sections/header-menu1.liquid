					{% for collection in collections %}
						<li class='with-sub-menu hover' data-item="{{collection.handle}}">
							<a href='/collections/{{collection.handle}}' class='clearfix'>
								<span>
									{% if collection.handle == 'mlb' %}
									<strong>Baseball</strong>
									{% elsif collection.handle == 'nfl' %}
									<strong>Football</strong>
									{% elsif collection.handle == 'nba' %}
									<strong>Basketball</strong>
									{% elsif collection.handle == 'nhl' %}
									<strong>Hockey</strong>
									{% elsif collection.handle == 'college' %}
									<strong>College</strong>
									{% else %}
									<strong>{{ collection.title }}</strong>
									{% endif %}
								</span>
							</a>
							
							<p class='close-menu'></p>
							<p class='open-menu'></p>
							{% if collection.all_tags.size > 24 %}
							<div class="sub-menu full-width-sub-menu" style="width:100%">
							{% elsif collection.all_tags.size > 16 and collection.all_tags.size < 24 %}
